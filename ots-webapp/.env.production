# Bluewud OTS - Production Environment Configuration
# Copy this file to .env and replace placeholders with actual values.

# === Application ===
VITE_APP_MODE=production
VITE_APP_NAME=Bluewud OTS

# === Zoho Catalyst ===
VITE_CATALYST_PROJECT_ID=your_catalyst_project_id
VITE_CATALYST_CLIENT_ID=your_catalyst_client_id

# === WhatsApp Business API ===
VITE_WHATSAPP_API_TOKEN=your_whatsapp_api_token
VITE_WHATSAPP_BUSINESS_ID=your_business_id
VITE_WHATSAPP_PHONE_ID=your_phone_id

# === Carrier APIs ===
VITE_DELHIVERY_API_TOKEN=your_delhivery_token
VITE_BLUEDART_LICENSE_KEY=your_bluedart_key
VITE_XPRESSBEES_API_KEY=your_xpressbees_key

# === Security ===
VITE_IP_WHITELIST_ENABLED=true
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key

# === Third-Party Integrations ===
VITE_FIXER_API_KEY=your_fixer_api_key

# === Feature Flags ===
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_ERROR_REPORTING=true
