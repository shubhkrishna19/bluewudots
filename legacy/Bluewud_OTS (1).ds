				 void Flipkart_Order_Data.Push_To_Order(Flipkart_Order_Data id)
                 {
                 	Get_Flipkart_Order = Flipkart_Order_Data[ID == id];
                 	orderMap = Map();
                 	for each  get_list in Get_Flipkart_Order
                 	{
                 		// Initialize empty list to hold matched customers
                 		get_customer = null;
                 		if(!get_list.Email_Id.isEmpty() && !get_list.Phone_No.isEmpty())
                 		{
                 			get_customer = Customer[Primary_Email == get_list.Email_Id || Phone_Number == get_list.Phone_No].ID;
                 		}
                 		else if(!get_list.Email_Id.isEmpty())
                 		{
                 			get_customer = Customer[Primary_Email == get_list.Email_Id].ID;
                 		}
                 		else if(!get_list.Phone_No.isEmpty())
                 		{
                 			get_customer = Customer[Phone_Number == get_list.Phone_No].ID;
                 		}
                 		if(get_customer != null)
                 		{
                 			Customer_ID = get_customer;
                 			updatecus = Customer[ID == get_customer];
                 			updatecus.Phone_Number=get_list.Phone_No;
                 			updatecus.Ship_to_Name=get_list.Ship_to_name;
                 			updatecus.Name=ifnull(get_list.Buyer_name,"Flipkart Customer");
                 		}
                 		else
                 		{
                 			Country = if(get_list.country == "IN","India",get_list.country);
                 			new_customer = insert into Customer
