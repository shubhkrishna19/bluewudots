				 void Amzon_order_data.Amazon_orderData(Amazon_Order_Data id)
                 {
                 	Get_amazon_order = Amazon_Order_Data[ID == id];
                 	orderMap = Map();
                 	// 	// to store unique orderId -> Order record ID
                 	for each  get_order in Get_amazon_order
                 	{
                 		// Initialize empty list to hold matched customers
                 		get_customer = null;
                 		if(!get_order.buyer_email.isEmpty() && !get_order.buyer_phone_number.isEmpty())
                 		{
                 			get_customer = Customer[Primary_Email == get_order.buyer_email || Phone_Number == get_order.buyer_phone_number].ID;
                 		}
                 		else if(!get_order.buyer_email.isEmpty())
                 		{
                 			get_customer = Customer[Primary_Email == get_order.buyer_email].ID;
                 		}
                 		else if(!get_order.buyer_phone_number.isEmpty())
                 		{
                 			get_customer = Customer[Phone_Number == get_order.buyer_phone_number].ID;
                 		}
                 		if(get_customer != null)
                 		{
                 			Customer_ID = get_customer;
                 			updatecus = Customer[ID == get_customer];
                 			updatecus.Phone_Number=get_order.buyer_phone_number;
                 		}
                 		else
                 		{
                 			Country = if(get_order.ship_country == "IN","India",get_order.ship_country);
                 			new_customer = insert into Customer
                 			[
                 				Added_User=zoho.loginuser
                 				Source="Amazon"
                 				Company_Name=get_order.buyer_company_name
                 				Amazon_Buyer_Name=ifnull(get_order.recipient_name,"No Name")
                 				Name=get_order.buyer_name
                 				Phone_Number=get_order.buyer_phone_number
                 				Amazon_Email=get_order.buyer_email
                 				Address.country=Country
                 				Address.district_city=get_order.ship_city
                 				Address.address_line_1=get_order.ship_address_1
                 				Address.address_line_2=get_order.ship_address_2
                 				Address.state_province=get_order.ship_state
                 				Address.postal_Code=get_order.ship_postal_code
                 			];
                 			Customer_ID = new_customer;
                 		}
                 		orderId = get_order.order_id;
                 		// --- Check if order already exists ---
                 		if(!orderMap.containKey(orderId))
                 		{
                 			// Insert order (only once per orderId)
                 			Country = if(get_order.ship_country == "IN","India",get_order.ship_country);
                 			if(Get_amazon_order.promise_date != null)
                 			{
                 				input_date_string = Get_amazon_order.promise_date;
                 				promise_date_new = input_date_string.toDate();
                 			}
                 			if(Get_amazon_order.purchase_date != null)
                 			{
                 				input_date_string_purchase_date = Get_amazon_order.purchase_date;
                 				purchase_date_new = input_date_string_purchase_date.toDate();
                 			}
                 			order_data = insert into Order
                 			[
                 				Added_User=zoho.loginuser
                 				Order_ID=orderId
