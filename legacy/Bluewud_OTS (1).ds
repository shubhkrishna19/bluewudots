			{
				type =  form
				form = POD
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											fet = Order[ID == input.Order];
										if(input.Order_Status == "Not Delivered")
										{
											count = fet.Rejection_Counter;
											if(count == null)
											{
												fet.Rejection_Counter=1;
											}
											else if(count < 3)
											{
												fet.Rejection_Counter=count + 1;
											}
											else if(count == 3)
											{
												fet.Status="Delivery Failed";
											}
										}
										else if(input.Order_Status == "Delivery Successful")
										{
											fet.Status="Delivery Confirmed";
										}
						)
					}
				}

			}
