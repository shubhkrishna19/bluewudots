				 void Flipkart_Order_Data.verifySKU(int id)
                 {
                 	Get_Flipkart_Order = Flipkart_Order_Data[ID == id];
                 	for each  get_order in Get_Flipkart_Order
                 	{
                 		if(get_order.Buyer_name == "")
                 		{
                 			UpdateRec = Flipkart_Order_Data[ID == get_order.ID];
                 			UpdateRec.Buyer_name="Flipkart Customer";
                 		}
                 		getproduct = Product[SKU_Code == get_order.SKU_Code];
                 		if(getproduct.count() == 0)
                 		{
                 			get_all_product = Product[ID != 0 && Product_Type == "Child"];
                 			for each  get_sku in get_all_product
                 			{
                 				if(get_order.SKU_Code.contains(get_sku.SKU_Code) == true)
                 				{
                 					Get_Flipkart_Order.SKU_Code=get_sku.SKU_Code;
                 					Get_Flipkart_Order.Upload_Status="SKU FOUNDED";
                 				}
                 				else if(get_order.SKU_Code.contains(get_sku.SKU_Code) == false)
                 				{
                 					Get_Flipkart_Order.Upload_Status="SKU MISSING";
                 				}
                 			}
                 		}
                 		else
                 		{
                 			update_status = Flipkart_Order_Data[ID == get_order.ID];
                 			update_status.Upload_Status="SKU FOUNDED";
                 		}
                 		order_details = Order[Order_ID == get_order.Order_Id];
                 		if(order_details.count() > 0)
                 		{
                 			delete from Flipkart_Order_Data[ID == get_order.ID];
                 		}
                 	}
                 }
