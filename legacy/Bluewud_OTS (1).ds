				 void Amzon_order_data.VERIFY_SKU(Amazon_Order_Data id)
                 {
                 	Get_amazon_order = Amazon_Order_Data[ID == id];
                 	for each  get_order in Get_amazon_order
                 	{
                 		getproduct = Product[SKU_Code == get_order.sku];
                 		if(getproduct.count() == 0)
                 		{
                 			get_all_product = Product[ID != 0 && Product_Type == "Child"];
                 			for each  get_sku in get_all_product
                 			{
                 				if(get_order.sku.contains(get_sku.SKU_Code))
                 				{
                 					// Update the Amazon order record
                 					update_order = Amazon_Order_Data[ID == get_order.ID];
                 					update_order.sku=get_sku.SKU_Code;
                 					update_order.Upload_Status="SKU FOUNDED";
                 				}
                 				else
                 				{
                 					// Update the Amazon order record
                 					update_order = Amazon_Order_Data[ID == get_order.ID];
                 					update_order.Upload_Status="SKU MISSING";
                 				}
                 			}
                 		}
                 		else
                 		{
                 			// SKU exists exactly
                 			update_status = Amazon_Order_Data[ID == get_order.ID];
                 			update_status.Upload_Status="SKU FOUNDED";
                 		}
                 		// Example: Delete Amazon order if it's already in the Order module
                 		order_details = Order[Order_ID == get_order.order_id];
                 		if(order_details.count() > 0)
                 		{
                 			delete from Amazon_Order_Data[ID == get_order.ID];
                 		}
                 	}
                 }
